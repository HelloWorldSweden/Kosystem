@page "/{RoomId:int}"
@inject RootState RootState

<Kosystem.Components.RoomComponent Room="_room"/>

@code {
    [Parameter]
    public int RoomId { get; set; }

    private RoomModel? _room;

    protected override void OnParametersSet()
    {
        _room = RootState.Rooms.TryGetById(RoomId);
    }
}
