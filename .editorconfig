root = true

[*]
end_of_line = lf
indent_style = space
insert_final_newline = true
indent_size = 4
trim_trailing_whitespace = true
charset = utf-8

[*.json]
indent_style = space
indent_size = 2

[*.cs]
# S1121: Assignments should not be made from within sub-expressions
dotnet_diagnostic.S1121.severity = silent

